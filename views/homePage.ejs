<script>
  // Wait for the DOM to be loaded
  document.addEventListener('DOMContentLoaded', function() {
    // Select the elements to be animated
    const titles = document.querySelectorAll('.title');
    const descriptions = document.querySelectorAll('.description');
    const images = document.querySelectorAll('.image');
    const shopButtons = document.querySelectorAll('.btn-brush');

    // Create animations for each element
    const titleAnimation = anime({
      targets: titles,
      opacity: [0, 1],
      translateY: [100, 0],
      duration: 1500, // Longer duration for smoother transition
      delay: anime.stagger(300), // Increased delay for smoother transition
    });

    const descriptionAnimation = anime({
      targets: descriptions,
      opacity: [0, 1],
      translateY: [100, 0],
      duration: 1500, // Longer duration for smoother transition
      delay: anime.stagger(300), // Increased delay for smoother transition
    });

    const imageAnimation = anime({
      targets: images,
      opacity: [0, 1],
      translateX: [-100, 0],
      duration: 8000, // Longer duration for smoother transition
      delay: anime.stagger(300), // Increased delay for smoother transition
    });

    const shopButtonAnimation = anime({
      targets: shopButtons,
      opacity: [0, 1],
      translateY: [100, 0],
      duration: 1500, // Duration for the shop button animation
      delay: anime.stagger(300), // Increased delay for smoother transition
    });

    // Stop the default Slick animation
    $('.hero-slider-1').slick('slickPause');

    // Function to change the active banner
    function changeBanner() {
      $('.hero-slider-1').slick('slickNext');
      setTimeout(() => {
        titleAnimation.restart();
        descriptionAnimation.restart();
        imageAnimation.restart();
        shopButtonAnimation.restart();
      }, 500); // Delay before restarting the animations (in milliseconds)
    }

    // Play the animations when the slider changes
    $('.hero-slider-1').on('afterChange', function() {
      titleAnimation.restart();
      descriptionAnimation.restart();
      imageAnimation.restart();
      shopButtonAnimation.restart();
    });

    // Automatically change banner after a specific time (in milliseconds)
    const intervalTime = 5000; // Change banner every 5 seconds
    setInterval(changeBanner, intervalTime);
  });
</script>

<script>
  // Initialize countdown timers
  const countdownElements = document.querySelectorAll('.deals-countdown');
  countdownElements.forEach((element) => {
    const countdownValue = element.getAttribute('data-countdown');
    startCountdown(element, countdownValue);
  });

  // Function to start the countdown timer
  function startCountdown(element, countdownValue) {
    const countdownDate = new Date(countdownValue).getTime();
    const countdownUpdate = setInterval(updateCountdown, 1000);

    function updateCountdown() {
      const now = new Date().getTime();
      const distance = countdownDate - now;

      // Calculations for days, hours, minutes, and seconds
      const days = Math.floor(distance / (1000 * 60 * 60 * 24));
      const hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
      const minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
      const seconds = Math.floor((distance % (1000 * 60)) / 1000);

      // Update the countdown element
      element.innerHTML = `${days}d ${hours}h ${minutes}m ${seconds}s`;

      // Check if the countdown has expired
      if (distance < 0) {
        clearInterval(countdownUpdate);
        element.innerHTML = 'Expired';
      }
    }
  }

  document.addEventListener('DOMContentLoaded', function() {
  const copyTags = document.querySelectorAll('.copy-tag');
  copyTags.forEach(function(copyTag) {
    copyTag.addEventListener('click', function() {
      const couponCodeText = this.parentNode.querySelector('.coupon-code-text');
      copyToClipboard(couponCodeText.innerText);
      showCopiedNotification();
    });
  });

  function copyToClipboard(text) {
    const textarea = document.createElement('textarea');
    textarea.value = text;
    textarea.style.position = 'fixed';
    textarea.style.opacity = 0;
    document.body.appendChild(textarea);
    textarea.select();
    document.execCommand('copy');
    document.body.removeChild(textarea);
  }

  function showCopiedNotification() {
    Swal.fire({
      icon: 'success',
      title: 'Coupon code copied!',
      showConfirmButton: false,
      timer: 1500
    });
  }
});

</script>


